'$meta stratum'(one, fact1)@0;
'$meta stratum'(one, rule1)@0;
'$meta stratum'(one, rule2)@0;
'$meta stratum'(one, test_passed)@0;



fact1(1)@0;
fact1(100)@0;
fact1(-10);

rule1(A, C) :-
  successor(A, B),
  fact1(B),
  successor(B, C);

rule2(A, E) :-
  fact1(A),
  successor(A, B),
  successor(B, C),
  successor(C, D),
  successor(D, E);



test_passed :-
  rule1(0, 2),
  rule1(99, 101),
  rule1(-11, -9),

  rule2(1, 5),
  rule2(100, 105),
  rule2(-10, -5);
